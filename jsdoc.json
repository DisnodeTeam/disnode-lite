{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ".",
        "includePattern": ".js$",
        "excludePattern": "(node_modules|docs|logs/|plugins/)"
    },
    "plugins": [

    ],
    "opts": {
        "template": "node_modules/ink-docstrap/template",
        "encoding": "utf8",
        "readme":"README.md",
        "destination": "docs/build",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "theme": "paper",
        "monospaceLinks": false
    },
    "docdash": {
        "static":false,
        "sort": false
    }
}
